# Copy this file to .env and fill in your actual key.
OPENAI_API_KEY=your-openai-api-key
OPENAI_TIMEOUT_SECONDS=120
DATABASE_URL=postgres://user:pass@localhost:5432/resume_analyzer?sslmode=disable

